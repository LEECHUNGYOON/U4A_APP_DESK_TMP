<?xml version='1.0' encoding='utf-8'?>
<widget id="com.U4A_APP_DESK_TMP.app" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>U4A_APP_DESK_TMP</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
     <platform name="electron">
        <preference name="AppendUserAgent" value="u4a_cordova_electron" />
        <preference name="ElectronSettingsFilePath" value="www/settings/u4a-electron-settings.json" />
        <icon src="www/img/logo.png" target="installer" />
    </platform>      
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
